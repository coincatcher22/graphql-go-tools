{
  "Vendor": true,
  "Deadline": "2m",
  "Sort": ["linter", "severity", "path", "line"],
  "EnableGC": true,
  "WarnUnmatchedDirective": true,
  "DisableAll": true,
  "Enable": [
    "deadcode",
    "gofmt",
    "golint",
    "gosimple",
    "ineffassign",
    "misspell",
    "unused",
    "vet"
  ],
  "Exclude": [
    "pkg/document/directivelocation_enum.go",
    "../../../../../../../usr/local/go/src/.."
  ]
}
