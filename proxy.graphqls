schema {
    query: Query
}

type Query {
    assets(first: Int): [Asset]
}

directive @RequestMiddleware on OBJECT_TYPE

type Asset implements Node @RequestMiddleware {
    status: Status!
    updatedAt: DateTime!
    createdAt: DateTime!
    id: ID!
    handle: String!
    fileName: String!
    height: Float
    width: Float
    size: Float
    mimeType: String
    url: String!
}
